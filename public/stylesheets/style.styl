  z-index 1000
  a
    text-decoration none
  .branding
    margin 12px auto
    display table
    p
      font 40px/50px 'LeagueG'
      color white
      text-transform uppercase
      span
        color #e24727
  .login,.signup,.username,.logout
    position absolute
    top 17px
    background-color rgba(225,225,225,.5)
    border 1px solid rgba(225,225,225,.6)
    color rgba(225,225,225,1)
    &:hover
      background-color rgba(0,0,0,.5)
    p
      font 25px/13px 'LeagueG'
      text-transform uppercase
      padding 13px 10px 0 10px
      color inherit
      -webkit-font-smoothing antialiased
      text-shadow 1px 1px 1px rgba(0,0,0,0.004)
  .login
    right 130px
    &:hover
      color rgba(225,225,225,.8)
  .signup
    right 25px
    &:hover
      //color rgb(239,78,68)
      color rgb(33,100,129)
  .username
    right 130px
    &:hover
      color rgba(225,225,225,.8)
  .logout
    right 25px
    &:hover
      cursor pointer
      color rgb(239,78,68)

